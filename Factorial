#include<stdio.h>

int main(){
    int n,i;
    long long fact=1;
    printf("Enter a number:");
    scanf("%d",&n);
    if(n<0){
        printf("Factorial is not possible.");
    }else{
         for(i=n;i>=1;i--){
             
            fact=fact*i;
             
         }
         printf("Factorial is:%lld",fact);
    }
   
 return 0;   
}
